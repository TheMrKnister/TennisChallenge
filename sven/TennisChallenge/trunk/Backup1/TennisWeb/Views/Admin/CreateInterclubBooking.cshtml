@model TennisWeb.Models.FullcalendarEventEditModel

@{
  ViewBag.Title = "Interklub Match Erstellen";
}

@section header
{
  @Styles.Render("~/Content/themes/base/jqueryui", "~/Content/fullcalendar")
}

@section scripts
{
  <script type="text/javascript">
    (function ($) {
      window.tc = window.tc || {}
      $.extend(window.tc, {
        calenderAPI: '@Url.HttpRouteUrl("EventFeedApi", new { controller = "EventFeed" })'
      })
    })(jQuery)
  </script>
  @Scripts.Render("~/bundles/interclub", "~/bundles/underscore", "~/bundles/jqueryui", "~/bundles/jqueryval", "~/bundles/fullcalendar")
}

<div id="interclub-booking">
  <div id="clear-field">
    @Html.ActionLink("Platz freigeben", "DeleteCurrentInterclubOnCourt", new { id = Guid.Empty }, new { id = "selected-court" })
  </div>

  @Html.Action("AdminMenu", new { adminNavigationCat = AdminNavigationCat.Interclub })

  @Html.Partial("_CreateBooking", Model, ViewData)
</div>
